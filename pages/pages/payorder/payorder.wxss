.pay-ui-row {
  color: #27323f;
  background-color: #fff;
}
.dhConponsBox {
  display: flex;
  align-items: center;
  width: 90%;
  margin: 0 auto;
  height: 100rpx;
}
.dhConponsBox .Ctitle {
  width: 130rpx;
}
.dhConponsBox input {
  width: 440rpx;
  color: #27323f;
  height: 80rpx;
}
.dhConponsBox .button {
  background-color: #27323f;
  color: #ffd305;
  padding: 4rpx 20rpx;
  margin-left: 8rpx;
  position: relative;
}
.dhConponsBox .button button {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  margin: 0;
  opacity: 0;
}
.youhuiquan-box {
  display: flex;
  justify-content: flex-end;
  flex-direction: row;
  align-items: center;
  color: #7b8196;
  font-size: 26rpx;
}
.pay-order-form {
  display: block;
  width: 100%;
  padding-bottom: 150rpx;
  background-color: #ffffff;
}
.ui-check-list {
  display: flex;
  align-items: center;
  height: 60rpx;
}
.ui-check-list switch {
  zoom: .8;
  padding-right: 10rpx;
}
.adds-center-bg {
  height: 70px;
  background-color: #fffaf9;
  color: #27323f;
}
.pay-adds-center-bg {
  color: #27323f;
  background-color: #fffaf9;
  border-top: 2rpx solid #f2f2f2;
}
.shops-ui-row {
  background-color: #fffaf9;
  border-top: 2rpx solid #f2f2f2;
}
.shops-ui-row .shops-name {
  color: #27323f;
  font-size: 28rpx;
}
.shops-ui-row .shops-name text {
  font-size: 22rpx;
  color: #7b8196;
  padding-left: 6rpx;
}
.shops-ui-row .shops-adds {
  color: #27323f;
  font-size: 22rpx;
}
.adds-icon {
  width: 42rpx;
  height: 42rpx;
  border: 2rpx solid #27323f;
  line-height: 40rpx;
}
.adds-class-line {
  width: 100%;
  height: 6rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100% 12rpx;
}
.pay-image-col {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.pay-image-col image {
  width: 150rpx;
  height: 150rpx;
  background-color: #f5f5f5;
  border-radius: 4rpx;
}
.pay-goods-col {
  display: block;
  width: 100%;
}
.pay-goods-name {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #27323f;
  width: 100%;
}
.pay-goods-label {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  color: #7b8196;
  width: 100%;
  font-size: 24rpx;
  padding-bottom: 10rpx;
}
.pay-goods-price {
  color: #a18d65;
}
.pay-goods-num {
  color: #27323f;
  padding-top: 20rpx;
}
.iphonex-bottom {
  height: 67rpx;
  background-color: #fff;
}
.fixed_bottom {
  color: #27323f;
  background: #fff;
  border-top: 2rpx solid #f5f5f5;
}
.fixed_bottom .form_list {
  background: #ffd305;
  color: #27323f;
  height: 50px;
  text-align: center;
  line-height: 50px;
  font-size: 32rpx;
  position: relative;
}
.fixed_bottom .form_list button {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
}
.fixed_bottom .form_list_none {
  background-color: #7b8196;
  color: #fff;
}
.accordion_content {
  background-color: #f5f5f5;
}
.accordion_content_row {
  border-bottom: 2rpx solid #e5e5e5;
}
.accordion_content_row_button {
  background-color: #27323f;
  color: #ffd305;
  font-size: 26rpx;
  border-radius: 2rpx;
  width: 160rpx;
  height: 62rpx;
  line-height: 62rpx;
  text-align: center;
}
.accordion_content_row_button_active {
  background-color: #ff4545;
  color: #fff;
  font-size: 26rpx;
  border-radius: 2rpx;
  width: 160rpx;
  height: 62rpx;
  line-height: 62rpx;
  text-align: center;
}
.address-active {
  color: #ffd305;
  font-size: 22rpx;
  padding: 2rpx 12rpx;
  border: 2rpx solid #ffd305;
  border-radius: 4rpx;
  margin-top: 12rpx;
}
.address-number {
  font-size: 30rpx;
  font-weight: 500;
}
.address-info {
  font-size: 24rpx;
  color: #7b8196;
  padding-top: 14rpx;
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.bottom20 {
  margin-bottom: 20rpx;
}
.ui-check-list-box {
  display: block;
  width: 100%;
}
.ui-check-list-box .radio-group {
  display: flex;
  flex-direction: column;
}
.ui-check-list-box .radio-group .radio {
  display: flex;
  height: 90rpx;
  align-items: center;
  border-bottom: 2rpx solid #f5f5f5;
}
.ui-check-list-box .radio-group .radio text {
  width: 95%;
}
.ui-check-list-box .ui-check-list .cells {
  background-color: #27323f;
}
.pay-ui-row-textarea {
  display: flex;
  width: 100%;
  height: 100%;
  align-items: center;
  justify-content: center;
}
.fixed-top-bottom {
  position: fixed;
  z-index: 99999;
  left: 0;
  bottom: 0;
}
.fixed_bottom {
  position: fixed !important;
  bottom: 0 !important;
  left: 0 !important;
  width: 100% !important;
}
.iphonexBottom {
  bottom: 67rpx !important;
}

.picker{
    padding: 13px;
    background-color: #FFFFFF;
}
