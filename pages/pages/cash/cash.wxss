.cash-header .user {
  display: flex;
  height: 100rpx;
  align-items: center;
  justify-content: center;
}
.cash-header .user image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 100%;
}
.cash-header .lable {
  display: flex;
  height: 110rpx;
  align-items: center;
  width: 90%;
  border-bottom: 2rpx solid #f5f5f5;
  margin: 0 auto;
  margin-top: 10rpx;
}
.cash-header .lable .name {
  color: #9ea2a6;
  font-size: 26rpx;
}
.cash-header .lable input {
  width: 570rpx;
  text-align: right;
  font-size: 80rpx;
  height: 80rpx;
  color: #273039;
}
.cash-header .lable .coupon {
  width: 590rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  color: #273039;
}
.cash-header .lable .uservip {
  width: 570rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  color: #273039;
}
.cash-header .lable .none {
  color: #9ea2a6;
}
.cash-header button {
  background-color: #39b571;
  color: #fff;
  width: 90%;
  margin-top: 50rpx;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  border-radius: 100rpx;
}
.cash-header button::after {
  border: 0;
}
.cash-header .money {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #273039;
  font-size: 28rpx;
  height: 90rpx;
  width: 90%;
  margin: 0 auto;
  margin-top: 36rpx;
  background-color: #fbfafc;
  border-radius: 100rpx;
}
.cash-header .money text {
  font-size: 24rpx;
  padding-left: 10rpx;
  color: #9ea2a6;
}
.balanceMask {
  width: 100%;
  height: 90vh;
  background-color: #39b571;
  border-top-left-radius: 28rpx;
  border-top-right-radius: 28rpx;
}
.balanceMask .user {
  width: 100%;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}
.balanceMask .user .userinfo-avatar {
  width: 120rpx;
  height: 120rpx;
  display: block;
  overflow: hidden;
  border-radius: 100%;
  position: absolute;
  top: -42rpx;
}
.balanceMask .user .close {
  position: absolute;
  left: 36rpx;
}
.balanceMask .user .refresh {
  position: absolute;
  right: 30rpx;
}
.balanceMask .qrcode {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 90%;
  background-color: #fff;
  margin: 0 auto;
  border-radius: 10rpx;
  margin-top: 30rpx;
  position: relative;
}
.balanceMask .qrcode .money {
  color: #273039;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 90rpx;
  width: 100%;
  background-color: #f2f2f2;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  margin-bottom: 30rpx;
}
.balanceMask .qrcode .money text {
  padding-left: 6rpx;
  font-weight: bold;
}
.balanceMask .qrcode .canvas {
  width: 380rpx;
  height: 380rpx;
  margin-top: 20rpx;
}
.balanceMask .qrcode .info {
  color: #9ea2a6;
  font-size: 22rpx;
  margin-bottom: 30rpx;
}
.balanceMask .recharge {
  position: absolute;
  bottom: 150rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.balanceMask .recharge .picker {
  width: 300rpx;
  height: 70rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 2rpx solid #fff;
  color: #fff;
}
.balanceMask .recharge .info {
  color: #fff;
  font-size: 22rpx;
  padding-top: 6rpx;
  padding-bottom: 6rpx;
}
.balanceMask .recharge .button {
  color: #39b571;
  font-size: 28rpx;
  background-color: #fff;
  padding: 8rpx 30rpx;
  border-radius: 100rpx;
  margin-top: 20rpx;
}
.couponMask {
  width: 100%;
  height: 75vh;
}
.couponMask .nameBox {
  width: 100%;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  border-bottom: 2rpx solid #f5f5f5;
}
.couponMask .nameBox .close {
  position: absolute;
  left: 36rpx;
}
.couponMask .couponBox {
  width: 100%;
  height: 65vh;
}
.couponMask .couponBox .list {
  display: flex;
  width: 90%;
  margin: 0 auto;
  height: 130rpx;
  border-radius: 6rpx;
  align-items: center;
  border: 2rpx solid #f2f2f2;
  margin-top: 30rpx;
}
.couponMask .couponBox .list .money {
  width: 180rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 46rpx;
  color: #39b571;
}
.couponMask .couponBox .list .money text {
  font-size: 28rpx;
  padding-top: 10rpx;
  padding-left: 2rpx;
}
.couponMask .couponBox .list .names {
  width: 340rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.couponMask .couponBox .list .names .name {
  color: #273039;
  font-size: 28rpx;
}
.couponMask .couponBox .list .names .text {
  color: #9ea2a6;
  font-size: 24rpx;
  padding-top: 4rpx;
}
.couponMask .couponBox .list .check {
  width: 150rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.couponMask .couponBox .list .check .button {
  background-color: #39b571;
  color: #fff;
  width: 130rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 52rpx;
  border-radius: 100rpx;
  font-size: 26rpx;
}
.couponMask .couponBox .list .check .check {
  background-color: #f76260;
}
.couponMask .noneImgTops {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 45vh;
}
.couponMask .noneImgTops image {
  width: 200rpx;
  height: 200rpx;
}
.couponMask .noneImgTops text {
  color: #999;
  padding-top: 20rpx;
}
.couponMask .getNewCoupon {
  width: 300rpx;
  height: 70rpx;
  margin: 0 auto;
  background-color: #f76260;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 50rpx;
  border-radius: 100rpx;
}
